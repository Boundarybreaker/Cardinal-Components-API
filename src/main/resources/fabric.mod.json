{
    "schemaVersion": 1,
    "environment": "*",
    "id": "cardinal_components",
    "name": "Cardinal Components API",
    "description": "dynamically exposing components",
    "version": "${version}",
    "side": "universal",
    "entrypoints": {
        "main": [
            "nerdhub.cardinal.components.CardinalComponents::init"
        ]
    },
    "custom": {
        "modmenu:api": true
    },
    "depends": {
        "fabric-api-base": "*"
    },
    "recommends": {
        "fabric-networking": "*"
    },
    "authors": [
        {
            "name": "UpcraftLP",
            "contact": {
                "homepage": "https://twitter.com/UpcraftLP"
            }
        }
    ],
    "contact": {
        "homepage": "https://minecraft.curseforge.com/projects/318449",
        "issues": "https://github.com/NerdHubMC/Cardinal-Components-API/issues",
        "sources": "https://github.com/NerdHubMC/Cardinal-Components-API"
    },
    "license": "MIT",
    "mixins": [
        {
            "environment": "*",
            "config": "mixins.cardinal_components.json"
        }
    ]
}
